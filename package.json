{
	"name": "helidon",
	"displayName": "Helidon",
	"description": "Helidon project generator",
	"version": "0.0.1",
	"icon": "images/helidon_logo_light.png",
	"author": "Oracle Corporation",
	"publisher": "Oracle",
	"license": "Apache-2.0",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"galleryBanner": {
		"color": "#b3d4fc",
		"theme": "light"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"keywords": [
		"java",
		"helidon",
		"maven"
	],
	"activationEvents": [
		"onCommand:helidon.generate",
		"onCommand:helidon.startPage",
		"onLanguage:java"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "Helidon generator configuration",
				"properties": {
					"helidon.showStartPage": {
						"type": "boolean",
						"default": true,
						"description": "If start page should be displayed on extention startup."
					}
				}
			}
		],
		"commands": [
			{
				"command": "helidon.generate",
				"title": "Helidon: Generate a Helidon project"
			},
			{
				"command": "helidon.startPage",
				"title": "Helidon: Start page"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}